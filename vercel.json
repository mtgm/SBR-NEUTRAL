{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/m/(.*)", "destination": "/index.html" },
    { "source": "/api/resolve", "destination": "/api/resolve.js" },
    { "source": "/file/(.*)", "destination": "/api/glb.js?key=$1" }
  ],
  "functions": {
    "api/resolve.js": {
      "includeFiles": "data/models.json"
    }
  }
}
